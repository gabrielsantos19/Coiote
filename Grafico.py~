def desenharEixos():
    #eixo x
    turtle.goto(pos_x - 10, pos_y)
    turtle.down()
    turtle.goto(pos_x + 310, pos_y)
    turtle.up()
    #eixo y
    turtle.goto(pos_x, pos_y - 10)
    turtle.down()
    turtle.goto(pos_x, pos_y + 210)
    turtle.up()
    pass


